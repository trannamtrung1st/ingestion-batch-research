{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "MqttClientOptions": {
    "Tcp": {
      "Server": "localhost",
      "NoDelay": true,
      "BufferSize": 40960
    },
    "KeepAlivePeriod": "00:01:00",
    "CleanSession": false,
    "SessionExpiryInterval": 600,
    "ReconnectDelaySecs": 15,
    "Topic": "$share/ahi/batch_ingestion",
    "QoS": 1
  }
}